pipeline {
	agent any
	
	stages {
		stage ('Compile Stage') {
		
			steps {
				sh '''
			cd HelloWorld
			mvn clean install
		'''
			}
		}
	post {
		always {
			junit 'demoStudent/target/surefire-reports/*.xml'
			
			}
		}


        stage ('Deployment Stage') {
            steps {
                echo 'deploying the application'
            }
        }
     }
}